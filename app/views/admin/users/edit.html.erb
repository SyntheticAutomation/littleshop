
  <%= form_for @user, url: admin_user_path(@user), html: {id: "user-form"} do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :address %>
    <%= f.text_field :address %>
    <%= f.label :city %>
    <%= f.text_field :city %>
    <%= f.label :state %>
    <%= f.text_field :state %>
    <%= f.label :zipcode %>
    <%= f.text_field :zipcode %>
    <%= f.label :email%>
    <%= f.text_field :email %>
    <%= f.submit %>
  <% end %>
