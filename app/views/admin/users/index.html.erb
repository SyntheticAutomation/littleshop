<div id="users">
  <% @users.each do |user| %>
  <div id="user-<%= user.id %>" class="card user" style="width: 18rem;">
    <div class="card-body">
      <h5 class="card-title"><%= link_to user.name, admin_user_path(user) %></h5>
      <h6 class="card-subtitle mb-2 text-muted">Registered Since: <%= user.created_at %></h6>
      <% if user.status %>
        <%= button_to "Disable", admin_user_path(user), method: :patch, class: "disable card-link" %>
      <% else %>
        <%= button_to "Enable", admin_user_path(user), method: :patch, class: "enable card-link" %>
      <% end %>
    </div>
  </div>
  <% end %>
</div>
