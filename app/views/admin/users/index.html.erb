<% @users.each do |user| %>
  <tr id="user-<%= user.id %>">
    Name: <td><%= link_to user.name, admin_user_path(user) %></td>
    Registered Since: <td><%= user.created_at %></td>
    <% if user.status %>
      <td><%= button_to "Disable", admin_user_path(user), method: :patch, class: "disable" %></td>
    <% else %>
      <td><%= button_to "Enable", admin_user_path(user), method: :patch, class: "enable" %></td>
    <% end %>
  </tr>
<% end %>
